{
  "applicationName": "Leafjs",
  "disqus": "",
  "googleAnalytics": "",
  "openGraph": {
    "title": "",
    "type": "website",
    "image": "",
    "site_name": "",
    "url": ""
  },
  "meta": {
    "title": "",
    "description": "",
    "keyword": ""
  },
  "linenums": true,
  "source": {
    "include": [
      "./lib/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "lenient": true,
    "destination": "./docs",
    "template": "./node_modules/jaguarjs-jsdoc"
  }
}
